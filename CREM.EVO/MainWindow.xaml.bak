                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="4*"/>
                                        <ColumnDefinition Width="1*"/>
                                    </Grid.ColumnDefinitions>
                                    <Border Grid.Column="1" BorderBrush="Gray" BorderThickness="5" d:IsHidden="True">
                                        <Grid Background="WhiteSmoke" d:IsHidden="True" >
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="1*"/>
                                                <RowDefinition Height="1*"/>
                                                <RowDefinition Height="1*"/>
                                            </Grid.RowDefinitions>
                                            <Button Command="{Binding BtnCmd}" CommandParameter="16" Style="{StaticResource ButtonStyle}" Grid.Row="0" Content="Add" Margin="10,35" d:IsHidden="True"></Button>
                                            <Button Command="{Binding BtnCmd}" CommandParameter="17" Style="{StaticResource ButtonStyle}" Grid.Row="1" Content="Del" Margin="10,35" d:IsHidden="True"></Button>
                                            <Button Command="{Binding BtnCmd}" CommandParameter="18" Style="{StaticResource ButtonStyle}" Grid.Row="2" Content="Save" Margin="10,35" d:IsHidden="True"></Button>
                                        </Grid>
                                    </Border>
                                    <Border Grid.Column="0" BorderBrush="Gray" BorderThickness="5" d:IsHidden="True">
                                        <Grid d:IsHidden="True">
                                            <TextBlock  Text="Name:" VerticalAlignment="Center" HorizontalAlignment="Left" Margin="10,10,0,350" d:IsHidden="True"></TextBlock>
                                            <TextBox  Text="{Binding _CrtWaterValve.Name, Mode=TwoWay}"  VerticalAlignment="Center" HorizontalAlignment="Left" Width="225" Margin="183,10,0,344" FontSize="24" d:IsHidden="True"></TextBox>
                                            <TextBlock  Text="IoAdress:" VerticalAlignment="Center" HorizontalAlignment="Left" Margin="10,83,0,277" d:IsHidden="True"></TextBlock>
                                            <TextBox  Text="{Binding _CrtWaterValve.DeviceIoAdress, Mode=TwoWay}"  VerticalAlignment="Center" HorizontalAlignment="Left" Width="225" Margin="183,83,0,271" FontSize="24" d:IsHidden="True"></TextBox>
