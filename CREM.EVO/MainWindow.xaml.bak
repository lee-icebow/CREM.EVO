<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:xx="clr-namespace:CREM.EVO"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d" x:Class="CREM.EVO.MainWindow"
       xmlns:xxx="clr-namespace:CREM.EVO.Utility;assembly=CREM.EVO.Utility"
     
    Title="EVO.PLATFORM" Height="600" Width="800">
    <Window.Resources>
        <xxx:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter"></xxx:BoolToVisibilityConverter>
        <xxx:IngredientTypeToVisibilityConverter x:Key="IngredientTypeToVisibilityConverter"></xxx:IngredientTypeToVisibilityConverter>
        <xxx:IntToIsChecked x:Key="IntToIsChecked"></xxx:IntToIsChecked>
        <xxx:IntToselectIndex x:Key="IntToselectIndex"></xxx:IntToselectIndex>
        <xxx:IntToPicScr x:Key="IntToPicScr"></xxx:IntToPicScr>
    </Window.Resources>
    <Viewbox Stretch="Fill" d:IsHidden="True" >
        <Grid Height="900" Width="1200" d:IsHidden="True">
            <Grid x:Name="Evo_Grd_MainSelect" Visibility="{Binding _UiData.is_mainselect_visible, Mode=TwoWay, Converter={StaticResource BoolToVisibilityConverter},ConverterParameter=Disabled}" d:IsHidden="True">
                <Grid Margin="200,0" d:IsHidden="True">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="1*"/>
                        <RowDefinition Height="1*"/>
                        <RowDefinition Height="1*"/>
                        <RowDefinition Height="1*"/>
                        <RowDefinition Height="1*"/>
                        <RowDefinition Height="1*"/>
                        <RowDefinition Height="1*"/>
                        <RowDefinition Height="1*"/>
                    </Grid.RowDefinitions>
                    <Button Command="{Binding BtnCmd}" CommandParameter="257" Style="{StaticResource MyButton}" Grid.Row="0" Margin="100,10" Content="Machine Setting" FontSize="36" d:IsHidden="True"/>
                    <Button Command="{Binding BtnCmd}" CommandParameter="258" Style="{StaticResource MyButton}" Grid.Row="1" Margin="100,10" Content="Machine Test" FontSize="36" d:IsHidden="True"/>
                    <Button Command="{Binding BtnCmd}" CommandParameter="259" Style="{StaticResource MyButton}" Grid.Row="2" Margin="100,10" Content="Recipe Setting System" FontSize="36" d:IsHidden="True"/>
                    <Button Command="{Binding BtnCmd}" CommandParameter="260" Style="{StaticResource MyButton}" Grid.Row="3" Margin="100,10" Content="Clean Setting System" FontSize="36" d:IsHidden="True"/>
                    <Button Command="{Binding BtnCmd}" CommandParameter="261" Style="{StaticResource MyButton}" Grid.Row="4" Margin="100,10" Content="DB System" FontSize="36" d:IsHidden="True"/>
                    <Button Command="{Binding ComCmd}" CommandParameter="513" Style="{StaticResource MyButton}" Grid.Row="6" Margin="100,10" Content="Communication Setting" FontSize="36" d:IsHidden="True"/>
                    <Button Command="{Binding BtnCmd}" CommandParameter="512" Style="{StaticResource MyButton}" Grid.Row="7" Margin="100,10" Content="Assist Tools" FontSize="36" d:IsHidden="True"/>
                    <Button Command="{Binding BtnCmd}" CommandParameter="263" Style="{StaticResource MyButton}" Grid.Row="5" Margin="100,10" Content="Machine Update" FontSize="36" d:IsHidden="True"/>

                </Grid>
            </Grid>
            <Grid x:Name="Evo_Grd_MachineSet" Visibility="{Binding _UiData.is_machineset_visible, Mode=TwoWay, Converter={StaticResource BoolToVisibilityConverter},ConverterParameter=Disabled}" d:IsHidden="True">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="3*"/>
                    <ColumnDefinition Width="8*"/>
                </Grid.ColumnDefinitions>
                <Border Grid.Column="0" BorderBrush="Gray" BorderThickness="3" d:IsHidden="True"/>
                <Border Grid.Column="1" BorderBrush="Gray" BorderThickness="3" d:IsHidden="True"/>
                <Grid Grid.Column="0" d:IsHidden="True">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="1*"/>
                        <RowDefinition Height="4*"/>
                        <RowDefinition Height="4*"/>
                    </Grid.RowDefinitions>
                    <Grid Grid.Row="0" d:IsHidden="True">
                        <Border Grid.Row="0" BorderBrush="Gray" BorderThickness="3" d:IsHidden="True"></Border>
                        <TextBlock Text="Select" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="32" d:IsHidden="True"></TextBlock>
                    </Grid>
                    <Grid Grid.Row="2" d:IsHidden="True">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="3*"/>
                            <ColumnDefinition Width="2*"/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="1*"/>
                            <RowDefinition Height="1*"/>
                            <RowDefinition Height="1*"/>
                            <RowDefinition Height="1*"/>
                            <RowDefinition Height="3*"/>
                        </Grid.RowDefinitions>
                        <Grid Grid.Row="4" Grid.ColumnSpan="2" Margin="20" d:IsHidden="True">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="1*"/>
                                <RowDefinition Height="1*"/>
                            </Grid.RowDefinitions>
                            <Button Command="{Binding BtnCmd}" CommandParameter="1" Style="{StaticResource ButtonStyle}" Grid.Row="0" Margin="20,5" Content="Save" FontSize="36" d:IsHidden="True"></Button>
                            <Button Command="{Binding BtnCmd}" CommandParameter="2" Style="{StaticResource ButtonStyle}" Grid.Row="2" Margin="20,5" Content="Back" FontSize="36" d:IsHidden="True"></Button>

                        </Grid>

                        <TextBlock Grid.Row="0" Grid.Column="0" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="24" Text="Valve" Margin="0,16" d:IsHidden="True"></TextBlock>
                        <TextBlock  Grid.Row="0" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="36" Text="{Binding _CrtEVOMachine.ValveAmt}" d:IsHidden="True" ></TextBlock>

                        <TextBlock Grid.Row="1" Grid.Column="0" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="24" Text="Mixser" Margin="0,16" d:IsHidden="True"></TextBlock>
                        <TextBlock  Grid.Row="1" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="36" Text="{Binding _CrtEVOMachine.MixerAmt}" d:IsHidden="True" ></TextBlock>

                        <TextBlock Grid.Row="2" Grid.Column="0" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="24" Text="Canister" Margin="0,16" d:IsHidden="True"></TextBlock>
                        <TextBlock  Grid.Row="2" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="36" Text="{Binding _CrtEVOMachine.CanisterAmt}" d:IsHidden="True" ></TextBlock>

                        <TextBlock Grid.Row="3" Grid.Column="0" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="24" Text="BeanHopper" Margin="0,16" d:IsHidden="True"></TextBlock>
                        <TextBlock  Grid.Row="3" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="36" Text="{Binding _CrtEVOMachine.BeanHopperAmt}" d:IsHidden="True" ></TextBlock>


                    </Grid>
                    <Grid Grid.Row="1" d:IsHidden="True">
                        <ListView ItemsSource="{Binding _LstEVOMachine}" Tag="Main" SelectionChanged="ListView_SelectionChanged" d:IsHidden="True">
                            <ListView.Template>
                                <ControlTemplate>
                                    <Border CornerRadius="10" BorderBrush="RoyalBlue" BorderThickness="5">
                                        <ScrollViewer VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Disabled" >
                                            <ItemsPresenter />
                                        </ScrollViewer>
                                    </Border>
                                </ControlTemplate>
                            </ListView.Template>
                            <ListView.ItemsPanel>
                                <ItemsPanelTemplate>
                                    <StackPanel IsItemsHost="True"/>
                                </ItemsPanelTemplate>
                            </ListView.ItemsPanel>
                            <ListView.ItemContainerStyle>
                                <Style TargetType="{x:Type ListViewItem}">
                                    <Setter Property="IsSelected" Value="{Binding IsSelected, Mode=TwoWay}"/>
                                    <Setter Property="Template">
                                        <Setter.Value>
                                            <ControlTemplate TargetType="{x:Type ListViewItem}">
                                                <Border Name="BD_Collector" CornerRadius="3" Background="DarkSeaGreen" Width="auto" Height="auto" Margin="5">
                                                    <Label Content="{Binding Path=Name}" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="36" />
                                                </Border>
                                                <ControlTemplate.Triggers>
                                                    <Trigger Property="IsMouseOver" Value="True">
                                                        <Setter TargetName="BD_Collector" Property="Background" Value="YellowGreen" />
                                                    </Trigger>
                                                    <Trigger Property="IsSelected" Value="True">
                                                        <Setter TargetName="BD_Collector" Property="Background" Value="DarkGreen" />
                                                    </Trigger>
                                                </ControlTemplate.Triggers>
                                            </ControlTemplate>
                                        </Setter.Value>
                                    </Setter>
                                </Style>
                            </ListView.ItemContainerStyle>
                        </ListView>
                    </Grid>
                </Grid>
